root = true

[*]
charset = utf-8
end_of_line = crlf
indent_size = 2
indent_size = space
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
indent_size = 4

[Jenkinsfile]
indent_size = 4

[*.sh]
end_of_line = lf

[node_modules/**]
charset = unset
end_of_line = unset
indent_size = unset
indent_style = unset
trim_trailing_whitespace = unset
insert_final_newline = false
